{
    "VARIABLES": [
        {
            "name": "x_i",
            "type": "INTEGER",
            "LaTeX": "x_i \\in \\mathbb{Z}^+",
            "description": "Quantity of Product I produced"
        },
        {
            "name": "x_ii",
            "type": "INTEGER",
            "LaTeX": "x_ii \\in \\mathbb{Z}^+",
            "description": "Quantity of Product II produced"
        },
        {
            "name": "x_iii",
            "type": "INTEGER",
            "LaTeX": "x_iii \\in \\mathbb{Z}^+",
            "description": "Quantity of Product III produced"
        }
    ],
    "OBJECTIVE": {
        "query": "sum(p_i * x_i, p_ii * x_ii, p_iii * x_iii) - sum(c_a1 * e_a1 * x_i, c_a2 * e_a2 * x_i, c_b1 * e_b1 * x_ii, c_b2 * e_b2 * x_iii, c_b3 * e_b3 * x_iii)",
        "goal": "MAX",
        "LaTeX": "\\max Z = \\sum_{i \\in {I, II, III}} p_i x_i - \\sum_{j \\in {A1, A2, B1, B2, B3}} c_j e_j x_j"
    },
    "CONSTRAINTS": [
        {
            "rule_id": "R1",
            "query": "x_i <= 1200",
            "LaTeX": "x_i \\le 1200",
            "description": "The total production of Product I can not exceed 1,200."
        },
        {
            "rule_id": "R2",
            "query": "x_i + x_iii <= 1510",
            "LaTeX": "x_i + x_iii \\le 1510",
            "description": "The total production of Product I and III can not exceed 1,510."
        },
        {
            "rule_id": "R3",
            "query": "x_ii <= 800",
            "LaTeX": "x_ii \\le 800",
            "description": "The total production of Product II can not exceed 800."
        },
        {
            "rule_id": "R4",
            "query": "x_i + x_ii + x_iii <= 1509",
            "LaTeX": "x_i + x_ii + x_iii \\le 1509",
            "description": "The total production of Product I, II, and III can not exceed 1,509."
        }
    ]
}