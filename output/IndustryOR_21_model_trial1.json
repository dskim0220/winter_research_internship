        { "name": "Supplier_B_Price", "query": "Supplier_B_Price", "LaTeX": "110" },
        { "name": "Supplier_C_Price", "query": "Supplier_C_Price", "LaTeX": "100" },
        { "name": "MinTables", "query": "MinTables", "LaTeX": "150" },
        { "name": "MaxTables", "query": "MaxTables", "LaTeX": "600" },
