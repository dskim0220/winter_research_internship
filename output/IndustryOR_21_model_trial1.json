    "OBJECTIVE": { "query": "Objective", "LaTeX": "120 x_{A} + 110 x_{B} + 100 x_{C}" },
    "CONSTRAINTS": [
        {
            "name": "MinTablesConstraint",
            "query": "MinTablesConstraint",
