        },
        {
            "name": "SumOfOrdersFromBAndC",
            "query": "SumOfOrdersFromBAndC",
            "LaTeX": "15 x_{B} + 15 x_{C} \geq 30"
        },
        {
            "name": "AtLeastSevenOrders",
            "query": "AtLeastSevenOrders",
            "LaTeX": "x_{A} + x_{B} + x_{C} \geq 7"
        },
        {
            "name": "MaxTablesFromA",
            "query": "MaxTablesFromA",
            "LaTeX": "x_{A} \leq 60"
        }
    ]
