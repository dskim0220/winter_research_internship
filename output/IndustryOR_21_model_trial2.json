\section*{Mathematical Model}

\subsection*{Sets}
\[
I = \{A, B, C\}, \quad J = \{1, 2, \ldots, 7\}
\]

\subsection*{Variables}
\[
\begin{aligned}
&x_{ij} \in \{0, 1\}, && \text{for } i \in I, j \in J \\
&y_{ij} \in \{0, 1\}, && \text{for } i \in I, j \in J \\
&z_{ij} \in \{0, 1\}, && \text{for } i \in I, j \in J
\end{aligned}
\]

\subsection*{Parameters}
\[
\begin{aligned}
&P_A = 120, && \text{(cost of ordering each table from Supplier A)} \\
&P_B = 110, && \text{(cost of ordering each table from Supplier B)} \\
&P_C = 100, && \text{(cost of ordering each table from Supplier C)} \\
&T_A = 20, && \text{(number of tables per order from Supplier A)} \\
&T_B = 15, && \text{(number of tables per order from Supplier B)} \\
&T_C = 15, && \text{(number of tables per order from Supplier C)} \\
&m_A = 150, && \text{(minimum number of tables to order from Supplier A)} \\
&m_{total} = 7, && \text{(minimum number of orders from Supplier A, B, or C)} \\
&m_A = 60, && \text{(maximum number of tables from Supplier A)} \\
&m_B = 30, && \text{(minimum number of tables to order from Supplier B if Supplier A is ordered)} \\
&m_C = 1, && \text{(minimum number of tables to order from Supplier C if Supplier B is ordered)} \\
&m_{AB} = 25, && \text{(minimum sum of tables from Supplier A and B)} \\
&m_{BC} = 30, && \text{(minimum sum of tables from Supplier B and C)} \\
&T_{total} = 600, && \text{(maximum total number of tables)}
\end{aligned}
\]

\subsection*{Objective Function}
\[
\min \sum_{i \in I} \sum_{j \in J} P_i \cdot x_{ij}
\]

