        { "name": "TOTAL_DUTY_HOURS", "query": "Total Duty Hours", "LaTeX": "\\sum_{i \\in \\text{STUDENTS}} \\sum_{d \\in \\text{DAYS}} x_{id} \\cdot \\text{WAGE}_{i,d} = 70" },
        { "name": "SUM_STUDENTS_5_6", "query": "Sum of Duty Hours for Students 5 and 6", "LaTeX": "\\sum_{d \\in \\text{DAYS}} x_{5,d} + \\sum_{d \\in \\text{DAYS}} x_{6,d} \\leq 14" },
        { "name": "STUDENT_5_THURSDAY", "query": "Student 5's Duty Hours on Thursday", "LaTeX": "\\sum_{d \\in \\text{DAYS}} x_{5,d} \\leq 2" }
