        { "name": "MIN_HOURS", "query": "Minimum Duty Hours", "LaTeX": "\\text{MIN_HOURS}_{\\text{1}} = 8, \\text{MIN_HOURS}_{\\text{2}} = 8, \\text{MIN_HOURS}_{\\text{3}} = 7, \\text{MIN_HOURS}_{\\text{4}} = 7, \\text{MIN_HOURS}_{\\text{5}} = 7, \\text{MIN_HOURS}_{\\text{6}} = 7" },
