{
    "PROBLEM_TYPE": "MIP",
    "SETS": "",
    "PARAMETERS": {
        "Prices": {
            "A1": 124,
            "A2": 109,
            "A3": 115
        },
        "ProductionCosts": {
            "A1": 73.3,
            "A2": 52.9,
            "A3": 65.4
        },
        "ActivationCosts": {
            "A1": 170000,
            "A2": 150000,
            "A3": 100000
        },
        "MinimumBatches": {
            "A1": 20,
            "A2": 20,
            "A3": 16
        },
        "Demand": {
            "A1": 5300,
            "A2": 4500,
            "A3": 5400
        },
        "Days": 22,
        "TotalProductionLimit": 1210,
        "ActivationTimeA1": 57
    },
    "VARIABLES": [
        {
            "Variable": "produce_A1",
            "Type": "Binary",
            "Unit": ""
        },
        {
            "Variable": "produce_A2",
            "Type": "Binary",
            "Unit": ""
        },
        {
            "Variable": "produce_A3",
            "Type": "Binary",
            "Unit": ""
        }
    ],
    "OBJECTIVE": "Maximize Revenue - Fixed Costs",
    "CONSTRAINTS": [
        {
            "Constraint": "TotalProductionLimit",
            "Type": "LessEqual",
            "Expression": "produce_A1 * 53 + produce_A2 * 45 + produce_A3 * 54 <= 1210"
        },
        {
            "Constraint": "ActivationRequirementA1",
            "Type": "GreaterEqual",
            "Expression": "produce_A1 >= 1 + ActivationTimeA1 / 60"
        },
        {
            "Constraint": "ProductionQuotas",
            "Type": "LessEqual",
            "Expression": "produce_A1 <= 500",
            "Comment": "A1 production quota"
        },
        {
            "Constraint": "ProductionQuotasA2",
            "Type": "LessEqual",
            "Expression": "produce_A2 <= 450",
            "Comment": "A2 production quota"
        },
        {
            "Constraint": "ProductionQuotasA3",
            "Type": "LessEqual",
            "Expression": "produce_A3 <= 550",
            "Comment": "A3 production quota"
        },
        {
            "Constraint": "Revenue",
            "Type": "Equal",
            "Expression": "produce_A1 * 53 * 124 + produce_A2 * 45 * 109 + produce_A3 * 54 * 115 - ActivationCosts['A1'] * produce_A1 - ActivationCosts['A2'] * produce_A2 - ActivationCosts['A3'] * produce_A3 - FixedCosts['A1'] * produce_A1 - FixedCosts['A2'] * produce_A2 - FixedCosts['A3'] * produce_A3"
        }
    ]
}