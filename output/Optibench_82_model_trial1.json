{
    "SETS": {
        "WORK_DAYS": "\\{\\text{Monday}, \\text{Tuesday}, \\text{Wednesday}, \\text{Thursday}, \\text{Friday}, \\text{Saturday}, \\text{Sunday}\\}",
        "EMPLOYEES": "\\{\\text{Employee}_1, \\text{Employee}_2, \\text{Employee}_3, \\text{Employee}_4, \\text{Employee}_5, \\text{Employee}_6, \\text{Employee}_7, \\text{Employee}_8, \\text{Employee}_9, \\text{Employee}_{10}\\}"
    },
    "PARAMETERS": [
        {
            "name": "EMPLOYEES_REQUIRED",
            "query": "EMPLOYEES_REQUIRED",
            "LaTeX": "\\{15, 13, 15, 18, 14, 16, 10\\}"
        },
        {
            "name": "MIN_SUNDAY_MONDAY_PAIR",
            "query": "MIN_SUNDAY_MONDAY_PAIR",
            "LaTeX": "3"
        },
        {
            "name": "DIFFERENCE_LIMIT",
            "query": "DIFFERENCE_LIMIT",
            "LaTeX": "7"
        }
    ],
    "VARIABLES": [
        {
            "name": "shift_assignment",
            "type": "Binary",
            "query": "shift_assignment",
            "LaTeX": "\\forall \\text{Employee} \\in \\text{EMPLOYEES}, \\forall \\text{Day} \\in \\text{WORK_DAYS}: x_{\\text{Employee}, \\text{Day}} \\in \\{0, 1\\}"
        },
        {
            "name": "employee_schedule",
            "type": "Continuous",
            "query": "employee_schedule",
            "LaTeX": "\\forall \\text{Employee} \\in \\text{EMPLOYEES}, \\forall \\text{Day} \\in \\text{WORK_DAYS}: y_{\\text{Employee}, \\text{Day}} \\in [0, 1]"
        }
    ],
    "OBJECTIVE": {
        "query": "minimize sum(shift_assignment[Employee] for Employee in EMPLOYEES)",
        "LaTeX": "\\min \\sum_{\\text{Employee} \\in \\text{EMPLOYEES}} x_{\\text{Employee}, \\text{Day}}"
    },
    "CONSTRAINTS": [
        {
            "name": "EmploymentRequirement",
            "query": "EmploymentRequirement",
            "LaTeX": "\\forall \\text{Day} \\in \\text{WORK_DAYS}: \\sum_{\\text{Employee} \\in \\text{EMPLOYEES}} x_{\\text{Employee}, \\text{Day}} \\cdot y_{\\text{Employee}, \\text{Day}} = \\text{EMPLOYEES_REQUIRED}[\\text{Day}]"
        },
        {
            "name": "SundayMondayPairConstraint",
            "query": "SundayMondayPairConstraint",
            "LaTeX": "\\sum_{\\text{Employee} \\in \\text{EMPLOYEES}} x_{\\text{Employee}, \\text{Sunday}} \\cdot x_{\\text{Employee}, \\text{Monday}} \\geq \\text{MIN_SUNDAY_MONDAY_PAIR}"
        },
        {
            "name": "DifferenceConstraint",
            "query": "DifferenceConstraint",
            "LaTeX": "\\left| \\sum_{\\text{Employee} \\in \\text{EMPLOYEES}} x_{\\text{Employee}, \\text{Monday}} - \\sum_{\\text{Employee} \\in \\text{EMPLOYEES}} x_{\\text{Employee}, \\text{Wednesday}} \\right| \\leq \\text{DIFFERENCE_LIMIT}"
        }
    ]
}