\section*{Mathematical Model}

\subsection*{Sets}
\[
I = \{A, B, C\}, \quad J = \{1, 2, \ldots, M\}
\]

\subsection*{Variables}
\[
\begin{aligned}
&x_{i,j} \in \{0, 1\}, && \text{for } i \in I, j \in J \\
&y_{i,j} \in \{0, 1\}, && \text{for } i \in I, j \in J \\
&z_{i,j} \in \{0, 1\}, && \text{for } i \in I, j \in J
\end{aligned}
\]

\subsection*{Parameters}
\[
\begin{aligned}
&P_{A} = 120, && \text{(cost of ordering each dining table from Supplier A)} \\
&P_{B} = 110, && \text{(cost of ordering each dining table from Supplier B)} \\
&P_{C} = 100, && \text{(cost of ordering each dining table from Supplier C)} \\
&T_{A} = 20, && \text{(number of tables per order from Supplier A)} \\
&T_{B} = 15, && \text{(number of tables per order from Supplier B)} \\
&T_{C} = 15, && \text{(number of tables per order from Supplier C)} \\
&M_{\text{min}} = 7, && \text{(minimum number of orders from Supplier A or B or C)} \\
&M_{\text{max}} = 600, && \text{(maximum number of tables)} \\
&M_{\text{min\_A}} = 150, && \text{(minimum number of tables from Supplier A)} \\
&M_{\text{max\_A}} = 60, && \text{(maximum number of tables from Supplier A)} \\
&M_{\text{min\_AB}} = 30, && \text{(minimum number of tables from Supplier B if Supplier A is ordered)} \\
&M_{\text{min\_BC}} = 30, && \text{(minimum number of tables from Supplier C if Supplier B is ordered)} \\
&M_{\text{min\_AC}} = 25, && \text{(minimum number of tables from Supplier A and B)} \\
&M_{\text{min\_B}} = 30, && \text{(minimum number of tables from Supplier B and C)}
\end{aligned}
\]

\subsection*{Objective Function}
\[
\min \sum_{i \in I} \sum_{j \in J} P_i \cdot x_{i,j}
\]

\subsection*{Constraints}
\begin{align*}
\sum_{i \in I} x_{i,j} &= 1, && \text{for all } j \in J \quad (\text{exactly one supplier per order}) \\
\sum_{j \in J} x_{i,j} &\leq M_{\text{max}}, && \text{for all } i \in I \quad (\text{total tables cannot exceed 600}) \\
\sum_{j \in J} y_{i,j} &= 1, && \text{for all } i \in I \quad (\text{exactly one order per supplier}) \\
\sum_{j \in J} z_{i,j} &= 1, && \text{for all } i \in I \quad (\text{exactly one supplier per order}) \\
\sum_{j \in J} x_{A,j} &\geq M_{\text{min\_A}}, && \text{(at least 150 tables from Supplier A)} \\
\sum_{j \in J} x_{A,j} &\leq M_{\text{max\_A}}, && \text{(no more than 60 tables from Supplier A)} \\
\sum_{j \in J} x_{A,j} + \sum_{j \in J} x_{B,j} &\geq M_{\text{min\_AC}}, && \text{(at least 25 tables from Supplier A and B)} \\
\sum_{j \in J} x_{B,j} + \sum_{j \in J} x_{C,j} &\geq M_{\text{min\_BC}}, && \text{(at least 30 tables from Supplier B and C)} \\
