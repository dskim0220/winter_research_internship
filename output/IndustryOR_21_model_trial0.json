    "VARIABLES": [
        {
            "name": "orders_A",
            "type": "Integer",
            "query": "orders_A",
            "LaTeX": "\\text{Let } x_{A} \\text{ be the number of orders from Supplier A}"
        },
        {
            "name": "orders_B",
            "type": "Integer",
            "query": "orders_B",
            "LaTeX": "\\text{Let } x_{B} \\text{ be the number of orders from Supplier B}"
        },
        {
            "name": "orders_C",
            "type": "Integer",
            "query": "orders_C",
            "LaTeX": "\\text{Let } x_{C} \\text{ be the number of orders from Supplier C}"
        }
    ],
    "OBJECTIVE": {
        "query": "Objective",
        "LaTeX": "\\text{Minimize } 120x_{A} + 110x_{B} + 100x_{C}"
    },
    "CONSTRAINTS": [
        {
            "name": "MinTablesConstraint",
            "query": "MinTablesConstraint",
            "LaTeX": "20x_{A} + 15x_{B} + 15x_{C} \\geq 150"
        },
        {
            "name": "MaxTablesConstraint",
            "query": "MaxTablesConstraint",
            "LaTeX": "20x_{A} + 15x_{B} + 15x_{C} \\leq 600"
        },
        {
            "name": "AtLeastOneOrderFromA",
            "query": "AtLeastOneOrderFromA",
            "LaTeX": "x_{A} \\geq 1"
        },
        {
            "name": "AtLeastThreeOrdersFromB",
            "query": "AtLeastThreeOrdersFromB",
            "LaTeX": "x_{B} \\geq 3"
        },
        {
            "name": "SupplierABDependent",
            "query": "SupplierABDependent",
            "LaTeX": "x_{B} \\geq M(1 - x_{A})"
        },
        {
            "name": "SupplierBCDependent",
            "query": "SupplierBCDependent",
            "LaTeX": "x_{C} \\geq M(1 - x_{B})"
        },
        {
            "name": "SumOfOrdersFromAAndB",
            "query": "SumOfOrdersFromAAndB",
            "LaTeX": "20x_{A} + 15x_{B} \\geq 25"
        },
        {
            "name": "SumOfOrdersFromBAndC",
            "query": "SumOfOrdersFromBAndC",
            "LaTeX": "15x_{B} + 15x_{C} \\geq 30"
        },
        {
            "name": "AtLeastSevenOrders",
            "query": "AtLeastSevenOrders",
            "LaTeX": "x_{A} + x_{B} + x_{C} \\geq 7"
        },
        {
            "name": "MaxTablesFromA",
            "query": "MaxTablesFromA",
            "LaTeX": "x_{A} \\leq 60"
        }
    ]
