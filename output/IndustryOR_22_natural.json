{
    "PROBLEM_TYPE": "MIP",
    "SETS": {
        "PRODUCTS": "A1, A2, A3",
        "DAYS": "1..22"
    },
    "PARAMETERS": {
        "MAX_DEMAND": {
            "A1": 5300,
            "A2": 4500,
            "A3": 5400
        },
        "PRICE": {
            "A1": 124,
            "A2": 109,
            "A3": 115
        },
        "COST": {
            "A1": 73.3,
            "A2": 52.9,
            "A3": 65.4
        },
        "QUOTA": {
            "A1": 500,
            "A2": 450,
            "A3": 550
        },
        "ACTIVATION_COST": {
            "A1": 170000,
            "A2": 150000,
            "A3": 100000
        },
        "MIN_BATCH": {
            "A1": 20,
            "A2": 20,
            "A3": 16
        },
        "TOTAL_PRODUCTION_LIMIT": 1210,
        "MIN_PROD_A1": 22
    },
    "VARIABLES": [
        {
            "Variable": "produce_A1",
            "Type": "Binary",
            "Unit": ""
        },
        {
            "Variable": "produce_A2",
            "Type": "Binary",
            "Unit": ""
        },
        {
            "Variable": "produce_A3",
            "Type": "Binary",
            "Unit": ""
        }
    ],
    "OBJECTIVE": "Maximize Revenue - Fixed Costs",
    "CONSTRAINTS": [
        {
            "Constraint": "Total Production Capacity",
            "Expression": "sum(DAYS, produce_A1 * 500 + produce_A2 * 450 + produce_A3 * 550) <= 1210"
        },
        {
            "Constraint": "Activation Cost Constraint",
            "Expression": "sum(PRODUCTS, produce_A1 * ACTIVATION_COST[A1] + produce_A2 * ACTIVATION_COST[A2] + produce_A3 * ACTIVATION_COST[A3]) <= 170000"
        },
        {
            "Constraint": "Minimum Production Batch",
            "Expression": "produce_A1 >= MIN_BATCH[A1], produce_A2 >= MIN_BATCH[A2], produce_A3 >= MIN_BATCH[A3]"
        },
        {
            "Constraint": "Production Line Availability",
            "Expression": "produce_A1 >= 57"
        },
        {
            "Constraint": "Demand Constraint",
            "Expression": "produce_A1 * MAX_DEMAND[A1] <= 5300, produce_A2 * MAX_DEMAND[A2] <= 4500, produce_A3 * MAX_DEMAND[A3] <= 5400"
        },
        {
            "Constraint": "Revenue and Cost",
            "Expression": "Revenue - FixedCosts = sum(PRODUCTS, PRICE[A] * produce_A[A] * 100 - COST[A] * produce_A[A] * 100)"
        }
    ]
}