{
    "PROBLEM_TYPE": "MIP",
    "SETS": {
        "Suppliers": "A, B, C"
    },
    "PARAMETERS": {
        "cost_A": 120,
        "cost_B": 110,
        "cost_C": 100,
        "min_tables": 150,
        "max_tables": 600,
        "min_orders_A": 7,
        "max_tables_A": 60,
        "min_tables_AB": 25,
        "min_tables_AC": 25,
        "min_tables_BC": 30
    },
    "VARIABLES": [
        {
            "name": "orders_A",
            "type": "Integer",
            "unit": "count"
        },
        {
            "name": "orders_B",
            "type": "Integer",
            "unit": "count"
        },
        {
            "name": "orders_C",
            "type": "Integer",
            "unit": "count"
        }
    ],
    "OBJECTIVE": "minimize cost_A*orders_A + cost_B*orders_B + cost_C*orders_C",
    "CONSTRAINTS": [
        {
            "name": "total_tables",
            "constraint": "orders_A*20 + orders_B*15 + orders_C*15 >= min_tables"
        },
        {
            "name": "max_tables_constraint",
            "constraint": "orders_A*20 + orders_B*15 + orders_C*15 <= max_tables"
        },
        {
            "name": "orders_A_min",
            "constraint": "orders_A >= min_orders_A"
        },
        {
            "name": "tables_from_A",
            "constraint": "orders_A >= min_tables_AB - orders_B"
        },
        {
            "name": "tables_from_B",
            "constraint": "orders_B >= min_tables_AC - orders_A"
        },
        {
            "name": "tables_from_C",
            "constraint": "orders_C >= min_tables_BC - orders_B"
        },
        {
            "name": "tables_from_A_or_B",
            "constraint": "orders_A + orders_B >= min_orders_A"
        },
        {
            "name": "tables_from_A_max",
            "constraint": "orders_A <= max_tables_A"
        }
    ]
}