{
    "PROBLEM_TYPE": "MIP",
    "SETS": "",
    "PARAMETERS": {
        "Prices": {
            "A1": 124,
            "A2": 109,
            "A3": 115
        },
        "Costs": {
            "A1": 73.3,
            "A2": 52.9,
            "A3": 65.4
        },
        "ActivationCosts": {
            "A1": 170000,
            "A2": 150000,
            "A3": 100000
        },
        "MinimumBatches": {
            "A1": 20,
            "A2": 20,
            "A3": 16
        },
        "Demand": {
            "A1": 5300,
            "A2": 4500,
            "A3": 5400
        },
        "TotalProductionLimit": 1210,
        "FixedActivationTime": 57
    },
    "VARIABLES": [
        {
            "Variable": "x_A1",
            "Type": "Continuous",
            "Unit": "tons"
        },
        {
            "Variable": "x_A2",
            "Type": "Continuous",
            "Unit": "tons"
        },
        {
            "Variable": "x_A3",
            "Type": "Continuous",
            "Unit": "tons"
        }
    ],
    "OBJECTIVE": "Maximize Revenue - Costs - FixedActivationCosts",
    "CONSTRAINTS": [
        {
            "Constraint": "TotalProductionLimit",
            "Type": "LessEqual",
            "Expression": "x_A1 + x_A2 + x_A3 <= 1210"
        },
        {
            "Constraint": "MinimumBatchA1",
            "Type": "GreaterEqual",
            "Expression": "x_A1 >= 20 * MinimumBatches['A1']"
        },
        {
            "Constraint": "MinimumBatchA2",
            "Type": "GreaterEqual",
            "Expression": "x_A2 >= 20 * MinimumBatches['A2']"
        },
        {
            "Constraint": "MinimumBatchA3",
            "Type": "GreaterEqual",
            "Expression": "x_A3 >= 16 * MinimumBatches['A3']"
        },
        {
            "Constraint": "ActivationTimeA1",
            "Type": "GreaterEqual",
            "Expression": "x_A1 >= FixedActivationTime / 60"
        },
        {
            "Constraint": "BinaryDecisionA1",
            "Type": "Binary",
            "Expression": "x_A1 >= 0.5 * x_A1 + 0.5 * MinimumBatches['A1']"
        }
    ]
}